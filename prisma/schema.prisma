generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String    @id @default(cuid())
  ref        String    @unique
  isApproved Boolean   @default(false)
  company    Company[]
}

model Company {
  id String @id @default(cuid())

  name String?

  userId String
  user   User   @relation(fields: [userId], references: [id])
}
